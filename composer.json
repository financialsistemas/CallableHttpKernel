{
    "name": "stack/callable-http-kernel",
    "description": "HttpKernelInterface implementation based on callables.",
    "keywords": [
        "stack"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/http-foundation": "~4.0",
        "symfony/http-kernel": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "Stack": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Stack": "test/unit"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "scripts": {
        "test": "phpunit --colors=always"
    }
}